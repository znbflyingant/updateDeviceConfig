{
  "buildCommand": "cd vue-frontend && npm run build",
  "outputDirectory": "vue-frontend/dist",
  "installCommand": "cd vue-frontend && npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
